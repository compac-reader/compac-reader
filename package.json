    "buffer": "^6.0.3",
    "cheerio": "^1.0.0-rc.10",
    "expo": "~49.0.21",
    "expo-asset": "~8.10.1",
    "expo-file-system": "~15.4.5",
    "expo-linear-gradient": "~12.3.0",
    "expo-sqlite": "~11.3.3",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-elements": "^3.4.2",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
